{
    "version": 2,
    "builds": [
      {
        "src": "PersonalWebpage/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      }
    
    ],
    "routes": [     
      {
        "src": "/static/(.*)",
        "dest": "PersonalWebpage/wsgi.py"
      }
    ]
  }